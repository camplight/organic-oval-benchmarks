{
  "extends": "standard",
  "ext": [".js", ".tag"],
  "rules": {
    "comma-dangle": [0]
  },
  "globals": {
    "describe": true,
    "before": true,
    "after": true,
    "it": true,
    "test": true,
    "expect": true,
    "xit": true,
    "xdescribe": true,
    "oval": true,
    "tag": true,
    "riot": true
  },
  "plugins": ["riot"]
}
